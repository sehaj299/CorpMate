{
  "kind": "collectionType",
  "collectionName": "resources",
  "info": {
    "singularName": "resource",
    "pluralName": "resources",
    "displayName": "Resource",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "profile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "email": {
      "type": "email",
      "required": true
    },
    "contact": {
      "type": "integer",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "inDepartment": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "inProject": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "isActive": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "companyId": {
      "type": "string",
      "required": true
    },
    "department": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::department.department",
      "inversedBy": "resources"
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "resources"
    }
  }
}
